{
  "navigationFallback": {
    "rewrite": "/",
    "exclude": ["/_next/static/*", "/_next/image*", "/api/*", "/healthz"]
  },
  "routes": [
    {
      "route": "/healthz",
      "allowedRoles": ["anonymous"],
      "serve": "/healthz.html",
      "statusCode": 200
    },
    {
      "route": "/_next/static/*",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "route": "/*.png",
      "headers": {
        "Cache-Control": "public, max-age=604800"
      }
    },
    {
      "route": "/*.svg",
      "headers": {
        "Cache-Control": "public, max-age=604800"
      }
    },
    {
      "route": "/*.ico",
      "headers": {
        "Cache-Control": "public, max-age=604800"
      }
    }
  ],
  "globalHeaders": {
    "X-Frame-Options": "DENY",
    "Referrer-Policy": "strict-origin-when-cross-origin",
    "X-Content-Type-Options": "nosniff"
  }
}
